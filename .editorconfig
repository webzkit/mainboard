# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = true

# Tab indentation
[*]
indent_style = space
indent_size = 4
trim_trailing_whitespace = true
insert_final_newline = true
max_line_length = 120

[*.{scss, ts, json, vue}]
indent_size = 2

[*.md]
trim_trailing_whitespace = false
